[user]
	email = atemu.main@gmail.com
	name = Atemu

[includeIf "gitdir/i:~/work/"]
	path = ~/work/.gitconfig

[merge]
	tool = vimdiff
[advice]
	detachedHead = off
[diff "mozlz4"]
	textconv = mozlz4a -d /dev/stdin /dev/stdout <
	binary = true
[diff "plist"]
	textconv = plutil -convert xml1 -o -
	binary = true

[annex]
	genmetadata = true
